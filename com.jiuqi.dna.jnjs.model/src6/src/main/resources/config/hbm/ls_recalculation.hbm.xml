<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping
		http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd">
    <class name="com.jiuqi.jnds.cfpl6.entity.LSRecalculation" table="ls_recalculation">
        <id name="id" type="uuid-binary" column="id" length="16"/>
        <property name="glid" type="uuid-binary" column="glid" length="16" not-null="false"/>
        <property name="unitname" type="string" column="unitname" not-null="false"/>
        <property name="unitcode" type="string" column="unitcode" not-null="false"/>
        <property name="acctyear" type="integer" column="acctyear" not-null="false"/>
        <property name="acctperiod" type="integer" column="acctperiod"  not-null="false"/>
        <property name="time" type="string" column="time" not-null="false"/>
    </class>
</hibernate-mapping>
